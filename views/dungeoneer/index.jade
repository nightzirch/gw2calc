extends ../layout

block content
	script(src="/js/dungeoneer.js")
	link(rel="stylesheet", href="/css/dungeoneer.css")
	
	ol.breadcrumb
		li
			a(href="/") Home
		li.active= title
	
	h1= title
	
	h2 How many dungeon tokens do you have?
	.dungeon-tokens
		.row.tokens
			.col-md-3
				.form-group
					.input-group
						input.form-control(placeholder="Ascalonian Tears", value="0", type="number", min="0", max="10290", step="1")
						.input-group-addon
							img(src="/img/Ascalonian_Tear.png", alt="Ascalonian Tears", title="Ascalonian Tears")
							span.dungeon-name Ascalonian Catacombs
			.col-md-3
				.form-group
					.input-group
						input.form-control(placeholder="Seals of Beetletun", value="0", type="number", min="0", max="10290", step="1")
						.input-group-addon
							img(src="/img/Seal_of_Beetletun.png", alt="Seals of Beetletun", title="Seals of Beetletun")
							span.dungeon-name Caudecus's Manor
			.col-md-3
				.form-group
					.input-group
						input.form-control(placeholder="Deadly Blooms", value="0", type="number", min="0", max="10290", step="1")
						.input-group-addon
							img(src="/img/Deadly_Bloom.png", alt="Deadly Blooms", title="Deadly Blooms")
							span.dungeon-name Twilight Arbor
			.col-md-3
				.form-group
					.input-group
						input.form-control(placeholder="Manifestos of the Moletariate", value="0", type="number", min="0", max="10290", step="1")
						.input-group-addon
							img(src="/img/Manifesto_of_the_Moletariate.png", alt="Manifestos of the Moletariate", title="Manifestos of the Moletariate")
							span.dungeon-name Sorrow's Embrace
		.row.tokens
			.col-md-3
				.form-group
					.input-group
						input.form-control(placeholder="Flame Legion Charr Carvings", value="0", type="number", min="0", max="10290", step="1")
						.input-group-addon
							img(src="/img/Flame_Legion_Charr_Carving.png", alt="Flame Legion Charr Carvings", title="Flame Legion Charr Carvings")
							span.dungeon-name Citadel of Flame
			.col-md-3
				.form-group
					.input-group
						input.form-control(placeholder="Symbols of Koda", value="0", type="number", min="0", max="10290", step="1")
						.input-group-addon
							img(src="/img/Symbol_of_Koda.png", alt="Symbols of Koda", title="Symbols of Koda")
							span.dungeon-name Honor of the Waves
			.col-md-3
				.form-group
					.input-group
						input.form-control(placeholder="Knowledge Crystals", value="0", type="number", min="0", max="10290", step="1")
						.input-group-addon
							img(src="/img/Knowledge_Crystal.png", alt="Knowledge Crystals", title="Knowledge Crystals")
							span.dungeon-name Crucible of Eternity
			.col-md-3
				.form-group
					.input-group
						input.form-control(placeholder="Shards of Zhaitan", value="0", type="number", min="0", max="10290", step="1")
						.input-group-addon
							img(src="/img/Shard_of_Zhaitan.png", alt="Shards of Zhaitan", title="Shards of Zhaitan")
							span.dungeon-name The Ruined City of Arah
	
